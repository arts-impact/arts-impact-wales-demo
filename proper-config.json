{
  "plugins": [],
  "dev-plugins": [],
  "themes": [
    {
      "name": "proper-bear",
      "url": "https://github.com/Bones5/Proper-Bear/archive/master.zip"
    }
  ],
  "migrateDbServers": [],
  "build": {
    "source": "themes/arts-impact",
    "buildTool": "webpack",
    "svg": {
      "srcDir": "_/svg/src",
      "destName": "symbols.svg",
      "destDir": "_/svg",
      "svgDocumentSettings": {
        "xmlDeclaration": true,
        "namespaceIDs": false,
        "doctypeDeclaration": true
      }
    },
    "js": {
      "srcDir": "_/js/src",
      "entrypoint": "index.js",
      "destName": "bundle.js",
      "destDir": "_/js"
    },
    "sass": {
      "srcDir": "_/scss",
      "destDir": ""
    }
  }
}